{
  "name": "create-electron-app",
  "version": "0.1.0",
  "homepage": "https://github.com/jubianchi/create-electron-app",
  "repository": {
    "type": "git",
    "url": "https://github.com/jubianchi/create-electron-app"
  },
  "license": "MIT",
  "author": {
    "name": "Julien Bianchi",
    "email": "contact@jubianchi.fr"
  },
  "bin": {
    "create-electron-app": "./scripts/main.js"
  },
  "scripts": {
    "build": "npm run build:${NODE_ENV:-${npm_package_config_default_build_environment:-development}}",
    "build:development": "webpack --config config/webpack.development.js",
    "build:production": "webpack --config config/webpack.production.js",
    "build:test": "NODE_ENV=test webpack --config config/webpack.test.js",
    "changelog": "conventional-changelog -i CHANGELOG.md -s -p ${npm_package_config_conventional_changelog_preset:-angular}",
    "cs": "prettier --config config/prettier.js --ignore-path .gitignore --write './**/*.{js,jsx,json,css,scss,sass,md,yml}' '!./**/package.json' '!./**/package-lock.json' ",
    "electron": "electron dist",
    "package": "npm run build:production && electron-builder -c config/electron-builder.js -mwl",
    "start": "npm-run-all -p start:renderer start:main",
    "start:main": "nodemon --watch config/webpack.development.js --watch config/webpack.server.js --exec 'node_modules/.bin/webpack  --config config/webpack.server.js --config-name main --watch'",
    "start:renderer": "nodemon --watch config/webpack.development.js --watch config/webpack.server.js --exec 'node_modules/.bin/webpack-dev-server --config config/webpack.server.js --config-name renderer'",
    "test": "npm-run-all build:test test:shared test:renderer test:main",
    "test:main": "NODE_ENV=test jest --coverage --coverageDirectory $(pwd)/coverage/main --config config/jest.main.js",
    "test:renderer": "NODE_ENV=test jest --config config/jest.renderer.js",
    "test:scripts": "NODE_ENV=test jest --coverage --coverageDirectory $(pwd)/coverage/scripts --clearMocks --rootDir $(pwd)/scripts '.*\\.spec.js'",
    "test:shared": "NODE_ENV=test jest --config config/jest.shared.js"
  },
  "config": {
    "conventional_changelog_preset": "angular",
    "default_build_environment": "development",
    "webpack_devserver_host": "0.0.0.0",
    "webpack_devserver_port": "9000"
  },
  "dependencies": {
    "chalk": "^3.0.0",
    "commander": "^2.17.1",
    "email-validator": "^2.0.4",
    "fs-extra": "^8.0.1",
    "inquirer": "^6.2.0",
    "inquirer-autocomplete-prompt": "^1.0.1",
    "semver": "^6.0.0",
    "sort-package-json": "^1.15.0",
    "spdx-license-list": "^6.0.0",
    "valid-url": "^1.0.9"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^2.0.2",
    "conventional-changelog-cli": "^2.0.21",
    "copy-webpack-plugin": "^5.0.3",
    "css-loader": "^2.1.1",
    "electron-builder": "^20.28.3",
    "electron-devtools-installer": "^2.2.4",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "eslint": "^5.4.0",
    "eslint-config-react-app": "^4.0.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-css-modules": "^2.8.1",
    "eslint-plugin-flowtype": "^3.9.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "html-webpack-plugin": "^3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.5.0",
    "node-sass": "^4.9.3",
    "nodemon": "^1.19.0",
    "npm-run-all": "^4.1.3",
    "prettier": "^1.17.1",
    "prettier-webpack-plugin": "^1.1.0",
    "react-hot-loader": "^4.3.5",
    "redux-mock-store": "^1.5.3",
    "sass-loader": "^7.1.0",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.16.5",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.7"
  },
  "optionalDependencies": {
    "bootstrap": "^4.1.3",
    "electron": "^5.0.1",
    "prop-types": "^15.6.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3",
    "redux": "^4.0.0"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=6"
  }
}
